{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "segments": [
        {
          "background": "#1E1E2E",
          "foreground": "#C0CAF5",
          "leading_diamond": "",
          "trailing_diamond": "",
          "style": "diamond",
          "template": "{{ if .SSHSession }} {{ end }} {{ .UserName }}",
          "type": "session"
        },
        {
          "type": "shell",
          "background": "#313244",
          "foreground": "#C0CAF5",
          "style": "diamond",
          "trailing_diamond": "",
          "leading_diamond": "",
          "template": " {{ .Name }}",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "PS",
              "zsh": "zsh",
              "bash": "bash"
            }
          }
        },
        {
          "type": "text",
          "style": "plain",
          "template": " 󰜥 "
        },
        {
          "type": "path",
          "background": "#89B4FA",
          "foreground": "#1E1E2E",
          "trailing_diamond": "",
          "leading_diamond": "",
          "style": "diamond",
          "properties": {
            "folder_icon": " 󰇘 ",
            "home_icon": " ",
            "style": "agnoster",
            "folder_separator_icon": ""
          },
          "template": "{{ .Path }}"
        },
        {
          "type": "git",
          "foreground": "#F5E0DC",
          "style": "plain",
          "template": "  "
        },
        {
          "type": "git",
          "background": "#F38BA8",
          "foreground": "#1E1E2E",
          "trailing_diamond": "",
          "leading_diamond": "",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": "{{ .HEAD }}{{ if gt .StashCount 0 }}\ueb4b {{ .StashCount }}{{ end }}"
        },
        {
          "type": "python",
          "foreground": "#F9E2AF",
          "style": "plain",
          "template": " 󰜥 "
        },
        {
          "type": "python",
          "background": "#A6E3A1",
          "foreground": "#1E1E2E",
          "trailing_diamond": "",
          "leading_diamond": "",
          "style": "diamond",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }} {{ .Venv }} {{ .Full }}{{ end }}{{ end }}",
          "properties": {
            "home_enabled": true,
            "display_mode": "always"
          }
        }
      ]
    },
    {
      "alignment": "left",
      "type": "prompt",
      "newline": true,
      "segments": [
        {
          "foreground": "#A6E3A1",
          "type": "text",
          "style": "plain",
          "template": " 󰜥 "
        }
      ]
    }
  ],

  "final_space": true,
  "version": 3
}
